<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">

    <ImageView
        android:id="@+id/setting_item_icon"
        style="@style/SettingItem"
        android:layout_alignParentLeft="true" />

    <TextView
        android:id="@+id/setting_item_title"
        style="@style/SettingItem.Title"
        android:textSize="14sp"
        android:layout_toRightOf="@id/setting_item_icon" />

    <ImageView
        android:id="@+id/setting_item_arrow"
        style="@style/SettingItem.Arrow"
        android:src="@drawable/list_icon_arrow_right" />

    <TextView
        android:id="@+id/setting_item_subtitle"
        style="@style/SettingItem"
        android:textSize="14sp"
        android:layout_toLeftOf="@id/setting_item_arrow"
        />
</merge>